{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/[product]/BreadCrumbs.jsx",
    "src/app/[product]/Description.jsx",
    "src/app/[product]/page.jsx",
    "src/app/[product]/Rating.jsx",
    "src/app/[product]/View.jsx",
    "src/app/cart/Cart.jsx",
    "src/app/cart/FindData.jsx",
    "src/app/cart/page.jsx",
    "src/app/checkout/Checkout.jsx",
    "src/app/checkout/Input.jsx",  // Fixed typo: 'Intput' -> 'Input'
    "src/app/checkout/Country.jsx",
    "src/app/components/achieve.jsx",  // Fixed typo: 'Achievem' -> 'achieve'
    "src/app/components/BrowseFeatures/Browse.jsx",  // Fixed typo: 'BrowsFeatures' -> 'BrowseFeatures'
    "src/app/components/BrowseFeatures/CardB.jsx",  // Fixed typo: 'CardB.jsx' path fixed
    "src/app/components/Cards/Card.jsx",
    "src/app/components/Footer/Footer.jsx",  // Fixed typo: 'Footer.j' -> 'Footer.jsx'
    "src/app/components/Furniture/Furniture.jsx",  // Fixed typo: 'Furnitur' -> 'Furniture'
    "src/app/components/Header/Nav.jsx",  // Fixed typo: 'Nav.jsx' typo in path
    "src/app/components/HeroSection/HeroSection.jsx",  // Fixed typo: 'Her' -> 'HeroSection'
    "src/app/components/ShoppingCard/ShoppingCard.jsx",  // Fixed typo: 'ShopingCard' -> 'ShoppingCard'
    "src/app/checkout/page.jsx",
    "src/app/contact/Contact.jsx",
    "src/app/contact/page.jsx",
    "src/app/DesktopCart/DesktopCart.jsx",
    "src/app/DesktopCart/DesktopBarCart.jsx",
    "src/app/favourite/Favourite.jsx",
    "src/app/favourite/page.jsx",
    "src/app/HeroBanner/HeroBanner.jsx",
    "src/app/profile/page.jsx",
    "src/app/shop/Filter.jsx",
    "src/app/shop/page.jsx",
    "src/app/store/StoreContext.jsx",
    "src/app/layout.jsx",
    "src/app/page.jsx"
, "src/app/studio/[[...tool]]/page.jsx"  ],
  "exclude": ["node_modules"]
}
